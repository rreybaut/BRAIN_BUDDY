  <div class="sound_record">
    <button id="startRecordingButton" class="btn btn-success">Start recording</button>
    <button id="stopRecordingButton" class="btn btn-success">Stop recording</button>
    <button id="playButton" class="btn btn-success">Play</button>
    <button id="downloadButton" class="btn btn-success">Download</button>
  </div>
  <div class="logos-container">
    <div class="logos">
      <%= image_tag "bb_micro.png", class: "A" %>
      <%= image_tag "bb_loupe.png", class: "logos B" %>
    </div>
  </div>
</div>
